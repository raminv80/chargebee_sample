<table>
  <thead>
    <tr>
      <th>Event id</th>
      <th>Source</th>
      <th>Type</th>
      <th>Created_at</th>
      <th>Data</th>
    </tr>
  </thead>
  <tbody>
  <% @events.each do |event| %>
  <tr>
    <td><%=event.event_id%></td>
    <td><%=event.event_source%></td>
    <td><%=event.event_type%></td>
    <td><%=event.created_at%></td>
    <td><%=event.event_data%></td>
  </tr>
  <% end %>
  </tbody>
</table>
